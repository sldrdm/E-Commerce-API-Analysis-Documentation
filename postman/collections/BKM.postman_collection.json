              "});\r",
              "\r",
              "// 2. Yanıt Süresi Testi (Senin yavaşlık tezin için!)\r",
              "pm.test(\"Yanıt süresi 800ms altında olmalı\", function () {\r",
              "    pm.expect(pm.response.responseTime).to.be.below(800);\r",
              "});\r",
              "\r",
              "// 3. Veri Yapısı Testi (Stok verisi geliyor mu?)\r",
              "var jsonData = pm.response.json();\r",
              "pm.test(\"İlk üründe stok_level alanı mevcut\", function () {\r",
              "    pm.expect(jsonData.res[0]).to.have.property('stock_level');\r",
              "});\r",
              "\r",
